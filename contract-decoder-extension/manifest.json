{
  "manifest_version": 3,
  "name": "Contract Decoder", 
  "version": "0.1.0",
  "description": "Minimal starter for Contract Decoder Chrome Extension.",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://generativelanguage.googleapis.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
  },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["modelApi/config.js", "modelApi/googleAI.js", "content.js"],
      "run_at":"document_idle"
    }
  ]
}
